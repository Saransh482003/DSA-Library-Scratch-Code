    "def SelectionSort(array):\n",
    "    for i in range(len(array)):\n",
    "        if array[i]>array[array.index(min(array[i:]))]:\n",
    "            s = array.index(min(array[i:]))\n",
    "            array[i],array[s]=array[s],array[i]\n",
    "    return array"
   ]
